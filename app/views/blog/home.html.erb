<p id="notice"><%= notice %></p>

<h1>Ted's blog</h1>

<%= render :partial => @current_post %>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
         <%= render :partial => post %>
      </tr>
    <% end %>
  </tbody>
</table>
